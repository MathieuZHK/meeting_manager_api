generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id             String    @unique @default(uuid())
  email          String?   @unique
  email_verified DateTime?
  name           String
  first_name     String
  password       String
  refresh_token  String?
  is_active      Boolean
}
